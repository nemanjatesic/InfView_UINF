{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "entities",
    "relations"
  ],
  "properties": {
    "entities": {
      "$id": "#/properties/entities",
      "type": "array",
      "title": "The Entities Schema",
      "items": {
        "$id": "#/properties/entities/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "name",
          "attributes"
        ],
        "properties": {
          "name": {
            "$id": "#/properties/entities/items/properties/name",
            "type": "string",
            "title": "The Name Schema",
            "default": "",
            "examples": [
              "CLAN"
            ],
            "pattern": "^(.*)$"
          },
          "attributes": {
            "$id": "#/properties/entities/items/properties/attributes",
            "type": "array",
            "title": "The Attributes Schema",
            "items": {
              "$id": "#/properties/entities/items/properties/attributes/items",
              "type": "object",
              "title": "The Items Schema",
              "required": [
                "name",
                "type",
                "length",
                "mandatory",
                "default",
                "primaryKey"
              ],
              "properties": {
                "name": {
                  "$id": "#/properties/entities/items/properties/attributes/items/properties/name",
                  "type": "string",
                  "title": "The Name Schema",
                  "default": "",
                  "examples": [
                    "ID_CLANA"
                  ],
                  "pattern": "^(.*)$"
                },
                "type": {
                  "$id": "#/properties/entities/items/properties/attributes/items/properties/type",
                  "type": "string",
                  "title": "The Type Schema",
                  "default": "",
                  "examples": [
                    "Varchar"
                  ],
                  "pattern": "^(.*)$"
                },
                "length": {
                  "$id": "#/properties/entities/items/properties/attributes/items/properties/length",
                  "type": "string",
                  "title": "The Length Schema",
                  "default": "",
                  "examples": [
                    "5"
                  ],
                  "pattern": "^(.*)$"
                },
                "mandatory": {
                  "$id": "#/properties/entities/items/properties/attributes/items/properties/mandatory",
                  "type": "string",
                  "title": "The Mandatory Schema",
                  "default": "",
                  "examples": [
                    "true"
                  ],
                  "pattern": "^(.*)$"
                },
                "default": {
                  "$id": "#/properties/entities/items/properties/attributes/items/properties/default",
                  "type": "string",
                  "title": "The Default Schema",
                  "default": "",
                  "examples": [
                    "null"
                  ],
                  "pattern": "^(.*)$"
                },
                "primaryKey": {
                  "$id": "#/properties/entities/items/properties/attributes/items/properties/primaryKey",
                  "type": "string",
                  "title": "The Primarykey Schema",
                  "default": "",
                  "examples": [
                    "true"
                  ],
                  "pattern": "^(.*)$"
                }
              }
            }
          }
        }
      }
    },
    "relations": {
      "$id": "#/properties/relations",
      "type": "array",
      "title": "The Relations Schema",
      "items": {
        "$id": "#/properties/relations/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "toEntity",
          "toAttribute",
          "fromEntity",
          "fromAttribute",
          "name"
        ],
        "properties": {
          "toEntity": {
            "$id": "#/properties/relations/items/properties/toEntity",
            "type": "string",
            "title": "The Toentity Schema",
            "default": "",
            "examples": [
              "ZADUZENJE"
            ],
            "pattern": "^(.*)$"
          },
          "toAttribute": {
            "$id": "#/properties/relations/items/properties/toAttribute",
            "type": "string",
            "title": "The Toattribute Schema",
            "default": "",
            "examples": [
              "ID_CLANA"
            ],
            "pattern": "^(.*)$"
          },
          "fromEntity": {
            "$id": "#/properties/relations/items/properties/fromEntity",
            "type": "string",
            "title": "The Fromentity Schema",
            "default": "",
            "examples": [
              "CLAN"
            ],
            "pattern": "^(.*)$"
          },
          "fromAttribute": {
            "$id": "#/properties/relations/items/properties/fromAttribute",
            "type": "string",
            "title": "The Fromattribute Schema",
            "default": "",
            "examples": [
              "ID_CLANA"
            ],
            "pattern": "^(.*)$"
          },
          "name": {
            "$id": "#/properties/relations/items/properties/name",
            "type": "string",
            "title": "The Name Schema",
            "default": "",
            "examples": [
              "relation 1"
            ],
            "pattern": "^(.*)$"
          }
        }
      }
    }
  }
}